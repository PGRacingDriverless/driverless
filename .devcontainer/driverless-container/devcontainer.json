{
    "name": "driverless",
    "dockerComposeFile": "${localWorkspaceFolder}/docker-compose.yml",
    "service": "driverless",
    "containerUser": "ros",
    "shutdownAction": "stopCompose",
    "workspaceFolder": "/home/ros/ws",
    "customizations": {
        "vscode": {
            "extensions":[
                "ms-vscode.cpptools",
                "ms-vscode.cpptools-themes",
                "twxs.cmake",
                "ms-iot.vscode-ros"
            ]
        }
    }
}